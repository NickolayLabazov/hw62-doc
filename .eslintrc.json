{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "off",
        "no-throw-literal": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-new": "off",
        "no-tabs": "off",
        "no-unused-vars": "off"
        
    }
}